{
  "name": "ngx-uptodate",
  "version": "0.0.0-development",
  "description": "a Github action that keeps your Angular CLI-based projects up-to-date with @angular via automated pull requests.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "stest": "jest --silent",
    "cover": "jest --coverage",
    "semantic-release": "semantic-release",
    "update": "npx npm-check-updates -u && npm install && npm audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinesoft/ngx-uptodate.git"
  },
  "homepage": "https://github.com/tinesoft/ngx-uptodate",
  "bugs": "https://github.com/tinesoft/ngx-uptodate/issues",
  "keywords": [
    "actions",
    "angular",
    "ng",
    "update"
  ],
  "author": "Tine Kondo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.0.1",
    "@actions/github": "^1.1.0",
    "@actions/io": "^1.0.1",
    "object-hash": "^2.0.0",
    "simple-git": "^1.126.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "del": "^5.1.0",
    "glob": "^7.1.5",
    "gulp": "^4.0.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "semantic-release": "^15.13.30",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime",
      "@babel/transform-async-to-generator"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
